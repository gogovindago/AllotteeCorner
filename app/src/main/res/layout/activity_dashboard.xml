<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:app="http://schemas.android.com/apk/res-auto">


    <androidx.appcompat.widget.LinearLayoutCompat xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        tools:context=".ui.DashboardActivity">

        <include
            android:id="@+id/toolbar"
            layout="@layout/toolbar_layout" />

        <androidx.swiperefreshlayout.widget.SwipeRefreshLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <ScrollView
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <androidx.appcompat.widget.LinearLayoutCompat
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">


                    <androidx.cardview.widget.CardView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="6dp"
                        android:layout_marginTop="8dp"
                        android:layout_marginEnd="6dp"
                        app:cardBackgroundColor="@color/white"
                        app:cardCornerRadius="8dp"
                        app:cardElevation="8dp"

                        app:cardMaxElevation="8dp"
                        app:cardPreventCornerOverlap="true"
                        app:cardUseCompatPadding="true">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:background="@drawable/edtbgwhite"
                            android:orientation="vertical">


                            <RelativeLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:background="@drawable/edtbgwhite"
                                android:orientation="horizontal">

                                <androidx.appcompat.widget.AppCompatTextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_centerVertical="true"
                                    android:layout_marginStart="8dp"
                                    android:gravity="center|start"
                                    android:text="Current Outstanding"
                                    android:textAllCaps="false"
                                    android:textAppearance="@style/TextAppearance.AppCompat.Small"
                                    android:textColor="@color/colorPrimary"
                                    android:textStyle="bold"
                                    android:visibility="visible"
                                    app:textAllCaps="false" />

                                <androidx.appcompat.widget.AppCompatTextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_alignParentEnd="true"
                                    android:layout_marginStart="8dp"
                                    android:layout_marginTop="8dp"
                                    android:layout_marginEnd="8dp"
                                    android:background="@drawable/shape_redbg"
                                    android:gravity="center|end"
                                    android:padding="4dp"
                                    android:text="Dues"
                                    android:textAllCaps="false"
                                    android:textColor="@color/white"
                                    android:textStyle="bold"
                                    android:visibility="visible"
                                    app:textAllCaps="false" />
                            </RelativeLayout>

                            <View
                                android:layout_width="match_parent"
                                android:layout_height="1dp"
                                android:background="@color/colorPrimary">

                            </View>


                            <LinearLayout
                                android:layout_width="wrap_content"

                                android:layout_height="wrap_content"
                                android:layout_marginTop="8dp"
                                android:baselineAligned="false"
                                android:orientation="horizontal">


                                <androidx.appcompat.widget.AppCompatTextView
                                    android:id="@+id/edtage"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_margin="8dp"
                                    android:drawablePadding="8dp"
                                    android:enabled="false"
                                    android:imeOptions="actionNext"
                                    android:text="95851.00"
                                    android:textColor="@color/black"
                                    android:textSize="35sp"
                                    android:textStyle="bold"
                                    app:layout_constraintEnd_toEndOf="parent"
                                    app:layout_constraintHorizontal_bias="0.0"
                                    app:layout_constraintStart_toStartOf="parent"
                                    app:layout_constraintTop_toBottomOf="@+id/textView7" />

                            </LinearLayout>


                            <RelativeLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:background="@drawable/edtbgwhite"
                                android:orientation="horizontal">

                                <androidx.appcompat.widget.AppCompatTextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_centerVertical="true"
                                    android:layout_marginStart="8dp"
                                    android:gravity="center|start"
                                    android:text="Total Current Outstanding"
                                    android:textAllCaps="false"
                                    android:textAppearance="@style/TextAppearance.AppCompat.Small"
                                    android:textColor="@color/colorPrimary"
                                    android:textStyle="bold"
                                    android:visibility="visible"
                                    app:textAllCaps="false" />

                                <androidx.appcompat.widget.AppCompatTextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_alignParentEnd="true"
                                    android:layout_marginStart="8dp"
                                    android:layout_marginTop="8dp"
                                    android:layout_marginEnd="8dp"
                                    android:gravity="center|end"
                                    android:padding="4dp"
                                    android:text="Dues"
                                    android:textAllCaps="false"
                                    android:textColor="@color/green"
                                    android:textStyle="bold"
                                    android:visibility="visible"
                                    app:textAllCaps="false" />
                            </RelativeLayout>

                        </LinearLayout>


                    </androidx.cardview.widget.CardView>

                    <androidx.cardview.widget.CardView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="6dp"
                        android:layout_marginTop="8dp"
                        android:layout_marginEnd="6dp"
                        app:cardBackgroundColor="@color/white"
                        app:cardCornerRadius="8dp"
                        app:cardElevation="8dp"
                        app:cardMaxElevation="8dp"
                        app:cardPreventCornerOverlap="true"
                        app:cardUseCompatPadding="true">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:background="@drawable/edtbgwhite"
                            android:orientation="vertical">


                            <RelativeLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:background="@drawable/edtbgwhite"
                                android:orientation="horizontal">

                                <androidx.appcompat.widget.AppCompatTextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_centerVertical="true"
                                    android:layout_marginStart="8dp"
                                    android:gravity="center|start"
                                    android:text="Future Outstanding"
                                    android:textAllCaps="false"
                                    android:textAppearance="@style/TextAppearance.AppCompat.Small"
                                    android:textColor="@color/colorPrimary"
                                    android:textStyle="bold"
                                    android:visibility="visible"
                                    app:textAllCaps="false" />

                                <androidx.appcompat.widget.AppCompatTextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_alignParentEnd="true"
                                    android:layout_marginStart="8dp"
                                    android:layout_marginTop="8dp"
                                    android:layout_marginEnd="8dp"
                                    android:background="@drawable/shape_greenbg"
                                    android:gravity="center|end"
                                    android:padding="4dp"
                                    android:text="Dues"
                                    android:textAllCaps="false"
                                    android:textColor="@color/white"
                                    android:textStyle="bold"
                                    android:visibility="visible"
                                    app:textAllCaps="false" />
                            </RelativeLayout>

                            <View
                                android:layout_width="match_parent"
                                android:layout_height="1dp"
                                android:background="@color/colorPrimary">

                            </View>


                            <LinearLayout
                                android:layout_width="wrap_content"

                                android:layout_height="wrap_content"
                                android:layout_marginTop="8dp"
                                android:baselineAligned="false"
                                android:orientation="horizontal">


                                <androidx.appcompat.widget.AppCompatTextView
                                    android:id="@+id/edtFutureOutstanding"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_margin="8dp"
                                    android:drawablePadding="8dp"
                                    android:enabled="false"
                                    android:imeOptions="actionNext"
                                    android:text="0.00"
                                    android:textColor="@color/black"
                                    android:textSize="35sp"
                                    android:textStyle="bold"
                                   />

                            </LinearLayout>


                            <RelativeLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:background="@drawable/edtbgwhite"
                                android:orientation="horizontal">

                                <androidx.appcompat.widget.AppCompatTextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_centerVertical="true"
                                    android:layout_marginStart="8dp"
                                    android:gravity="center|start"
                                    android:text="Total Future Outstanding"
                                    android:textAllCaps="false"
                                    android:textAppearance="@style/TextAppearance.AppCompat.Small"
                                    android:textColor="@color/colorPrimary"
                                    android:textStyle="bold"
                                    android:visibility="visible"
                                    app:textAllCaps="false" />

                                <androidx.appcompat.widget.AppCompatTextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_alignParentEnd="true"
                                    android:layout_marginStart="8dp"
                                    android:layout_marginTop="8dp"
                                    android:layout_marginEnd="8dp"
                                    android:gravity="center|end"
                                    android:padding="4dp"
                                    android:text="pending"
                                    android:textAllCaps="false"
                                    android:textColor="@color/blue"
                                    android:textStyle="bold"
                                    android:visibility="visible"
                                    app:textAllCaps="false" />
                            </RelativeLayout>

                        </LinearLayout>


                    </androidx.cardview.widget.CardView>

                    <androidx.cardview.widget.CardView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="6dp"
                        android:layout_marginTop="8dp"
                        android:layout_marginEnd="6dp"
                        app:cardBackgroundColor="@color/white"
                        app:cardCornerRadius="8dp"
                        app:cardElevation="8dp"
                        app:cardMaxElevation="8dp"
                        app:cardPreventCornerOverlap="true"
                        app:cardUseCompatPadding="true">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:background="@drawable/edtbgwhite"
                            android:orientation="vertical">


                            <RelativeLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:background="@drawable/edtbgwhite"
                                android:orientation="horizontal">

                                <androidx.appcompat.widget.AppCompatTextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_centerVertical="true"
                                    android:layout_marginStart="8dp"
                                    android:gravity="center|start"
                                    android:text="Plot Status"
                                    android:textAllCaps="false"
                                    android:textAppearance="@style/TextAppearance.AppCompat.Small"
                                    android:textColor="@color/colorPrimary"
                                    android:textStyle="bold"
                                    android:visibility="visible"
                                    app:textAllCaps="false" />

                                <androidx.appcompat.widget.AppCompatTextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_alignParentEnd="true"
                                    android:layout_marginStart="8dp"
                                    android:layout_marginTop="8dp"
                                    android:layout_marginEnd="8dp"
                                    android:background="@drawable/shape_bluebg"
                                    android:gravity="center|end"
                                    android:padding="4dp"
                                    android:text="Status "
                                    android:textAllCaps="false"
                                    android:textColor="@color/white"
                                    android:textStyle="bold"
                                    android:visibility="visible"
                                    app:textAllCaps="false" />
                            </RelativeLayout>

                            <View
                                android:layout_width="match_parent"
                                android:layout_height="1dp"
                                android:background="@color/colorPrimary">

                            </View>


                            <LinearLayout
                                android:layout_width="wrap_content"

                                android:layout_height="wrap_content"
                                android:layout_marginTop="8dp"
                                android:baselineAligned="false"
                                android:orientation="horizontal">


                                <androidx.appcompat.widget.AppCompatTextView
                                    android:id="@+id/edtAllotted"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_margin="8dp"
                                    android:drawablePadding="8dp"
                                    android:enabled="false"
                                    android:imeOptions="actionNext"
                                    android:text="Allotted"
                                    android:textColor="@color/black"
                                    android:textSize="35sp"
                                    app:layout_constraintEnd_toEndOf="parent"
                                    app:layout_constraintHorizontal_bias="0.0"
                                    app:layout_constraintStart_toStartOf="parent"
                                    app:layout_constraintTop_toBottomOf="@+id/textView7" />

                            </LinearLayout>


                            <RelativeLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:background="@drawable/edtbgwhite"
                                android:orientation="horizontal">

                                <androidx.appcompat.widget.AppCompatTextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_centerVertical="true"
                                    android:layout_marginStart="8dp"
                                    android:gravity="center|start"
                                    android:text="Total Future Outstanding"
                                    android:textAllCaps="false"
                                    android:textAppearance="@style/TextAppearance.AppCompat.Small"
                                    android:textColor="@color/colorPrimary"
                                    android:textStyle="bold"
                                    android:visibility="visible"
                                    app:textAllCaps="false" />

                                <androidx.appcompat.widget.AppCompatTextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_alignParentEnd="true"
                                    android:layout_marginStart="8dp"
                                    android:layout_marginTop="8dp"
                                    android:layout_marginEnd="8dp"
                                    android:gravity="center|end"
                                    android:padding="4dp"
                                    android:text="Status "
                                    android:textAllCaps="false"
                                    android:textColor="@color/blue"
                                    android:textStyle="bold"
                                    android:visibility="visible"
                                    app:textAllCaps="false" />
                            </RelativeLayout>

                        </LinearLayout>


                    </androidx.cardview.widget.CardView>


                    <androidx.cardview.widget.CardView
                        android:id="@+id/cardsforadoptedpersondetailrural"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="6dp"
                        android:layout_marginTop="8dp"
                        android:layout_marginEnd="6dp"
                        android:layout_marginRight="6dp"
                        app:cardBackgroundColor="@color/colorPrimary"
                        app:cardCornerRadius="8dp"
                        app:cardElevation="8dp"
                        app:cardMaxElevation="8dp"
                        app:cardPreventCornerOverlap="true"
                        app:cardUseCompatPadding="true">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:background="@drawable/edtbgwhite"
                            android:orientation="vertical">

                            <androidx.appcompat.widget.AppCompatTextView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center_horizontal"
                                android:gravity="center"
                                android:text="---- Property Details -----"
                                android:textAllCaps="false"
                                android:textAppearance="@style/TextAppearance.AppCompat.Medium"
                                android:textColor="@color/colorPrimary"
                                android:textStyle="bold"
                                android:visibility="visible"
                                app:textAllCaps="false" />

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:baselineAligned="false"
                                android:orientation="horizontal"
                                android:weightSum="1">


                                <com.google.android.material.textfield.TextInputLayout
                                    style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox.Dense"
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"

                                    android:layout_marginStart="16dp"
                                    android:layout_marginTop="1dp"
                                    android:layout_marginEnd="16dp"
                                    android:layout_weight="0.5">

                                    <com.google.android.material.textfield.TextInputEditText
                                        android:id="@+id/edtName"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:clickable="false"
                                        android:drawableStart="@drawable/person"
                                        android:drawableLeft="@drawable/person"
                                        android:drawablePadding="16dp"
                                        android:editable="true"
                                        android:enabled="true"
                                        android:hint="Urban Estate"
                                        android:imeOptions="actionNext"
                                        android:inputType="textPersonName"
                                        android:textColor="@color/black"
                                        android:textSize="12sp"
                                        android:textStyle="bold"
                                        app:layout_constraintEnd_toEndOf="parent"
                                        app:layout_constraintHorizontal_bias="0.0"
                                        app:layout_constraintStart_toStartOf="parent"
                                        app:layout_constraintTop_toBottomOf="@+id/textView7"
                                        tools:ignore="TooDeepLayout" />
                                </com.google.android.material.textfield.TextInputLayout>

                                <com.google.android.material.textfield.TextInputLayout
                                    style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox.Dense"
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"

                                    android:layout_marginStart="16dp"
                                    android:layout_marginTop="1dp"
                                    android:layout_marginEnd="16dp"
                                    android:layout_weight="0.5">

                                    <com.google.android.material.textfield.TextInputEditText
                                        android:id="@+id/edcontactno"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:clickable="false"
                                        android:digits="0123456789"

                                        android:drawablePadding="16dp"
                                        android:editable="true"
                                        android:enabled="true"
                                        android:hint="Sector"
                                        android:imeOptions="actionNext"
                                        android:inputType="phone"
                                        android:maxEms="10"
                                        android:maxLength="10"
                                        android:textColor="@color/black"
                                        android:textSize="12sp"
                                        android:textStyle="bold"
                                        app:layout_constraintEnd_toEndOf="parent"
                                        app:layout_constraintHorizontal_bias="0.0"
                                        app:layout_constraintStart_toStartOf="parent"
                                        app:layout_constraintTop_toBottomOf="@+id/textView7" />
                                </com.google.android.material.textfield.TextInputLayout>
                            </LinearLayout>


                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:baselineAligned="false"
                                android:orientation="horizontal"
                                android:weightSum="1">


                                <com.google.android.material.textfield.TextInputLayout
                                    style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox.Dense"
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"

                                    android:layout_marginStart="16dp"
                                    android:layout_marginTop="1dp"
                                    android:layout_marginEnd="16dp"
                                    android:layout_weight="0.5">

                                    <com.google.android.material.textfield.TextInputEditText
                                        android:id="@+id/edtCategory"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:clickable="false"
                                        android:drawableStart="@drawable/person"
                                        android:drawableLeft="@drawable/person"
                                        android:drawablePadding="16dp"
                                        android:editable="true"
                                        android:enabled="true"
                                        android:hint="Category"
                                        android:imeOptions="actionNext"
                                        android:inputType="textPersonName"
                                        android:textColor="@color/black"
                                        android:textSize="12sp"
                                        android:textStyle="bold"

                                        tools:ignore="TooDeepLayout" />
                                </com.google.android.material.textfield.TextInputLayout>

                                <com.google.android.material.textfield.TextInputLayout
                                    style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox.Dense"
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"

                                    android:layout_marginStart="16dp"
                                    android:layout_marginTop="1dp"
                                    android:layout_marginEnd="16dp"
                                    android:layout_weight="0.5">

                                    <com.google.android.material.textfield.TextInputEditText
                                        android:id="@+id/edtSubCategory"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:clickable="false"
                                        android:drawablePadding="16dp"
                                        android:editable="true"
                                        android:enabled="true"
                                        android:hint="Sub-Category"
                                        android:imeOptions="actionNext"
                                        android:textColor="@color/black"
                                        android:textSize="12sp"
                                        android:textStyle="bold"
                                       />
                                </com.google.android.material.textfield.TextInputLayout>
                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:baselineAligned="false"
                                android:orientation="horizontal"
                                android:weightSum="1">


                                <com.google.android.material.textfield.TextInputLayout
                                    style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox.Dense"
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"

                                    android:layout_marginStart="16dp"
                                    android:layout_marginTop="1dp"
                                    android:layout_marginEnd="16dp"
                                    android:layout_weight="0.5">

                                    <com.google.android.material.textfield.TextInputEditText
                                        android:id="@+id/edtPlotNo"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:clickable="false"
                                        android:drawableStart="@drawable/person"
                                        android:drawableLeft="@drawable/person"
                                        android:drawablePadding="16dp"
                                        android:editable="true"
                                        android:enabled="true"
                                        android:hint="Plot No"
                                        android:imeOptions="actionNext"
                                        android:inputType="textPersonName"
                                        android:textColor="@color/black"
                                        android:textSize="12sp"
                                        android:textStyle="bold"
                                        tools:ignore="TooDeepLayout" />
                                </com.google.android.material.textfield.TextInputLayout>

                                <com.google.android.material.textfield.TextInputLayout
                                    style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox.Dense"
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:layout_marginStart="16dp"
                                    android:layout_marginTop="1dp"
                                    android:layout_marginEnd="16dp"
                                    android:layout_weight="0.5">

                                    <com.google.android.material.textfield.TextInputEditText
                                        android:id="@+id/edtArea"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:clickable="false"
                                        android:drawablePadding="16dp"
                                        android:editable="true"
                                        android:enabled="true"
                                        android:hint="Area"
                                        android:imeOptions="actionNext"
                                        android:textColor="@color/black"
                                        android:textSize="12sp"
                                        android:textStyle="bold"
                                        app:layout_constraintEnd_toEndOf="parent"
                                        app:layout_constraintHorizontal_bias="0.0"
                                        app:layout_constraintStart_toStartOf="parent"
                                        app:layout_constraintTop_toBottomOf="@+id/textView7" />
                                </com.google.android.material.textfield.TextInputLayout>
                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal"
                                android:weightSum="1">


                                <com.google.android.material.textfield.TextInputLayout
                                    style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox.Dense"
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:layout_marginStart="16dp"
                                    android:layout_marginTop="1dp"
                                    android:layout_marginEnd="16dp"
                                    android:layout_weight="1">

                                    <com.google.android.material.textfield.TextInputEditText
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:clickable="false"
                                        android:drawablePadding="16dp"
                                        android:editable="true"
                                        android:enabled="true"
                                        android:hint="Status"
                                        android:imeOptions="actionNext"
                                        android:inputType="text"
                                        android:textColor="@color/black"
                                        android:textSize="12sp"
                                        android:textStyle="bold"/>
                                </com.google.android.material.textfield.TextInputLayout>

                            </LinearLayout>



                        </LinearLayout>


                    </androidx.cardview.widget.CardView>


                </androidx.appcompat.widget.LinearLayoutCompat>
            </ScrollView>
        </androidx.swiperefreshlayout.widget.SwipeRefreshLayout>

    </androidx.appcompat.widget.LinearLayoutCompat>
</layout>
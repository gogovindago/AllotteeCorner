                        android:layout_height="match_parent"
                        android:layout_weight="0.2"
                        android:drawablePadding="5dp"
                        android:gravity="center|start"
                        android:padding="5dp"
                        android:text="PermanentAddress1 "
                        android:textAppearance="@android:style/TextAppearance.StatusBar.EventContent"
                        android:textColor="@android:color/black"
                        android:textStyle="normal"
                        tools:targetApi="lollipop" />

                    <TextView
                        android:id="@+id/txtPermanentAddress1value"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_alignParentEnd="true"
                        android:layout_alignParentRight="true"
                        android:layout_weight="0.2"
                        android:background="@drawable/bg_boder"
                        android:gravity="center"
                        android:marqueeRepeatLimit="marquee_forever"
                        android:padding="8dp"
                        android:text="N/A"
                        android:textAllCaps="true"
                        android:textAppearance="@android:style/TextAppearance.StatusBar.EventContent"
                        android:textColor="@android:color/black"
                        android:textSize="12sp"
                        android:textStyle="normal"
                        tools:targetApi="lollipop" />

                </LinearLayout>


                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/bg_boder"
                    android:orientation="horizontal">

                    <TextView

                        android:id="@+id/txtSemester"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_gravity="center"
                        android:layout_weight="0.2"
                        android:gravity="center|start"
                        android:padding="8dp"
                        android:text="PermanentAddress2"
                        android:textAppearance="@android:style/TextAppearance.StatusBar.EventContent"
                        android:textColor="@android:color/black" />

                    <TextView
                        android:id="@+id/txtPermanentAddress2value"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_gravity="center"
                        android:layout_weight="0.2"
                        android:background="@drawable/bg_boder"
                        android:gravity="center"
                        android:padding="8dp"
                        android:text="N/A"
                        android:textAppearance="@android:style/TextAppearance.StatusBar.EventContent"

                        android:textColor="@android:color/black" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/bg_boder"
                    android:orientation="horizontal"
                    android:visibility="visible">

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_gravity="center"
                        android:layout_weight="0.2"
                        android:gravity="center|start"
                        android:padding="8dp"
                        android:text="PermanentAddress3 "
                        android:textAppearance="@android:style/TextAppearance.StatusBar.EventContent"
                        android:textColor="@android:color/black"
                        tools:targetApi="lollipop" />

                    <TextView
                        android:id="@+id/textPermanentAddress3value"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_gravity="center"
                        android:layout_weight="0.2"
                        android:background="@drawable/bg_boder"
                        android:gravity="center"
                        android:padding="8dp"
                        android:text="N/A"
                        android:textAppearance="@android:style/TextAppearance.StatusBar.EventContent"
                        android:textColor="@android:color/black"
                        tools:targetApi="lollipop" />
                </LinearLayout>


                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/bg_boder"
                    android:orientation="horizontal"
                    android:visibility="visible">

                    <TextView
                        android:id="@+id/textEndDate"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_weight="0.2"
                        android:padding="8dp"
                        android:text="Permanent State "
                        android:textAppearance="@android:style/TextAppearance.StatusBar.EventContent"
                        android:textColor="@android:color/black"
                        android:textStyle="normal" />

                    <TextView
                        android:id="@+id/textPermanentStatevalue"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_gravity="center"
                        android:layout_weight="0.2"
                        android:background="@drawable/bg_boder"
                        android:gravity="center_horizontal"
                        android:padding="8dp"
                        android:text="N/A"
                        android:textAppearance="@android:style/TextAppearance.StatusBar.EventContent"
                        android:textColor="@android:color/black"
                        android:textStyle="normal" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/bg_boder"
                    android:orientation="horizontal"
                    android:visibility="visible">

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_weight="0.2"
                        android:padding="8dp"
                        android:text="PermanentPIN"
                        android:textAppearance="@android:style/TextAppearance.StatusBar.EventContent"
                        android:textColor="@android:color/black"
                        android:textStyle="normal" />

                    <TextView
                        android:id="@+id/txtPermanentPINvalue"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_weight="0.2"
                        android:background="@drawable/bg_boder"
                        android:gravity="center_horizontal"
                        android:padding="8dp"
                        android:text="N/A"
                        android:textAppearance="@android:style/TextAppearance.StatusBar.EventContent"
                        android:textColor="@android:color/black"
                        android:textStyle="normal" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/bg_boder"
                    android:orientation="horizontal"
                    android:visibility="visible">

                    <TextView

                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_weight="0.2"
                        android:padding="8dp"
                        android:text="CorrespondenceAddress1"
                        android:textAppearance="@android:style/TextAppearance.StatusBar.EventContent"
                        android:textColor="@android:color/black"
                        android:textStyle="normal" />

                    <TextView
                        android:id="@+id/textCorrespondenceAddress1Value"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_weight="0.2"
                        android:background="@drawable/bg_boder"
                        android:gravity="center_horizontal"
                        android:padding="8dp"
                        android:text="N/A"
                        android:textAllCaps="true"
                        android:textAppearance="@android:style/TextAppearance.StatusBar.EventContent"
                        android:textColor="@android:color/black"
                        android:textStyle="normal" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/bg_boder"
                    android:orientation="horizontal"
                    android:visibility="visible">

                    <TextView

                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_weight="0.2"
                        android:padding="8dp"
                        android:text="CorrespondenceAddress2"
                        android:textAppearance="@android:style/TextAppearance.StatusBar.EventContent"
                        android:textColor="@android:color/black"
                        android:textStyle="normal" />

                    <TextView
                        android:id="@+id/textCorrespondenceAddress2Value"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_gravity="center"
                        android:layout_weight="0.2"
                        android:background="@drawable/bg_boder"
                        android:gravity="center"
                        android:padding="8dp"
                        android:text="N/A"
                        android:textAllCaps="true"
                        android:textAppearance="@android:style/TextAppearance.StatusBar.EventContent"
                        android:textColor="@android:color/black"
                        android:textStyle="normal" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/bg_boder"
                    android:orientation="horizontal"
                    android:visibility="visible">

                    <TextView

                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_weight="0.2"
                        android:padding="8dp"
                        android:text="CorrespondenceAddress3"
                        android:textAppearance="@android:style/TextAppearance.StatusBar.EventContent"
                        android:textColor="@android:color/black"
                        android:textStyle="normal" />

                    <TextView
                        android:id="@+id/textCorrespondenceAddress3Value"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_gravity="center"
                        android:layout_weight="0.2"
                        android:background="@drawable/bg_boder"
                        android:gravity="center"
                        android:padding="8dp"
                        android:text="N/A"
                        android:textAllCaps="true"
                        android:textAppearance="@android:style/TextAppearance.StatusBar.EventContent"
                        android:textColor="@android:color/black"
                        android:textStyle="normal" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/bg_boder"
                    android:orientation="horizontal"
                    android:visibility="visible">

                    <TextView

                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_weight="0.2"
                        android:padding="8dp"
                        android:text="CorrespondenceState"
                        android:textAppearance="@android:style/TextAppearance.StatusBar.EventContent"
